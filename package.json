{
  "name": "code-action-register",
  "displayName": "Code Action Register",
  "version": "0.0.2",
  "description": "A tool to register code-action",
  "publisher": "Fireworks",
  "engines": {
    "vscode": "^1.75.0"
  },
  "repository": {
    "type": "git",
